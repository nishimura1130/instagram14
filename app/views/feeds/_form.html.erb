
  <%= form_with(model: feed, local: true, url:confirm_feeds_path) do |form| %>
  <div class="field">
    <%= form.label :image %>
    <!-- 確認画面から新規作成画面に戻った時に、 -->
    <!-- アップロードした画像を表示させるための記述 -->
    <%= form.file_field :image %>
    <%= form.hidden_field :image_cache %>
    <%= form.text_area :comment %>
  </div>
  <div class="actions">
    <%= form.submit %>
  </div>

<% end %>
